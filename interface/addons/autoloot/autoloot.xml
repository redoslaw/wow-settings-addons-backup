<Frame name="Main_Frame" parent="UIParent" toplevel="true" enableMouse="true" hidden="true" movable="true" clampedToScreen = "true">
<Scripts>
 <OnMouseDown>
  if button == "LeftButton" and not self.isMoving then
   self:StartMoving();
   self.isMoving = true;
  end
 </OnMouseDown>
 <OnMouseUp>
  if button == "LeftButton" and self.isMoving then
   self:StopMovingOrSizing();
   self.isMoving = false;
  end
 </OnMouseUp>
 <OnHide>
  if ( self.isMoving ) then
   self:StopMovingOrSizing();
   self.isMoving = false;
  end
 </OnHide>
</Scripts>

