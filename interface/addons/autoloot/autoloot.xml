		<Size>
			<AbsDimension x="30" y="30"/>
		</Size>
		<Anchors>
			<Anchor point="TOPLEFT"/>
		</Anchors>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="autoloot_minimap_button_icon" file="Interface\AddOns\autoloot\textures\minimap-normal.tga">
					<Size>
						<AbsDimension x="26" y="26"/>
					</Size>
					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset>
								<AbsDimension x="3" y="-2"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="OVERLAY">
				<Texture file="Interface\Minimap\MiniMap-TrackingBorder">
					<Size>
						<AbsDimension x="52" y="52"/>
					</Size>
					<Anchors>
						<Anchor point="TOPLEFT"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Frames>
			<Frame name="autoloot_minimap_button_dragging_frame" hidden="true">
				<Scripts>
					<OnUpdate>
						autoloot_minimap_button_on_update()
					</OnUpdate>
				</Scripts>
			</Frame>
		</Frames>
		<HighlightTexture alphaMode="ADD" file="Interface\Minimap\UI-Minimap-ZoomButton-Highlight"/>
		<Scripts>
			<OnLoad>
				autoloot_minimap_button:RegisterForClicks("LeftButtonUp","MiddleButtonDown")
				autoloot_minimap_button:RegisterForDrag("RightButton")
				autoloot_minimap_button:RegisterEvent("PLAYER_ENTERING_WORLD");
			</OnLoad>
			<OnDragStart>
			autoloot_minimap_button:LockHighlight()
			autoloot_minimap_button_dragging_frame:Show()
			</OnDragStart>
			<OnDragStop>
				autoloot_minimap_button:UnlockHighlight()
				autoloot_minimap_button_dragging_frame:Hide()
			</OnDragStop>
			<OnClick> minimap_eventhandler() </OnClick>
			<OnEvent> autoloot_minimapbutton_Reposition() </OnEvent>
			<OnEnter> autoloot_minimap_button_on_enter() </OnEnter>
			<OnLeave> GameTooltip:Hide() </OnLeave>
		</Scripts>
	</Button>
	
   
