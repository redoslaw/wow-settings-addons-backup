<Ui xmlns="http://www.blizzard.com/wow/ui/"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	   
<Script file="autoloot-lib.lua" />

	<Frame name="lootwindow" parent="UIParent" toplevel="true" enableMouse="true" movable="true" clampedToScreen = "true" hidden = "true">
    <Size x="200" y="600"/>
    <Anchors>
        <Anchor point="CENTER"/>
    </Anchors>
	<Scripts>
		<OnMouseDown>
			if button == "LeftButton" and not self.isMoving then
			self:StartMoving();
			self.isMoving = true;
			end
		</OnMouseDown>
		<OnMouseUp>
			if button == "LeftButton" and self.isMoving then
			self:StopMovingOrSizing();
			self.isMoving = false;
			end
		</OnMouseUp>
		<OnHide>
			if ( self.isMoving ) then
			self:StopMovingOrSizing();
			self.isMoving = false;
			end
		</OnHide>
	</Scripts>
    <Backdrop bgFile="Interface\TutorialFrame\TutorialFrameBackground"
       edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
	<EdgeSize val="16"/>
	<TileSize val="32"/>
 	<BackgroundInsets left="5" right="5" top="5" bottom="5"/>
    </Backdrop>
	
	<Layers>
	 <Layer level = "ARTWORK">
	 
	 <FontString name="info_text" font="fonts\ARIALN.ttf" text="BROKEN TEXT" justifyH="LEFT" justifyV="TOP" parent = "lootwindow">
		<Color r="1" g="1" b="1" a="1"/>
		<FontHeight val="16"/>
		<Anchors>
			<Anchor point="CENTER" relativeTo="lootwindow" x="0" y="100" />
		</Anchors>
		<Shadow>
			<Offset x="1" y="1" />
			<Color r="0" g="0" b="0"/>
		</Shadow>
	  </FontString>
	  
	  <FontString name="fade_in_text" font="fonts\ARIALN.ttf" text="BROKEN TEXT" justifyH="LEFT" justifyV="TOP" parent = "lootwindow">
		<Color r="1" g="1" b="1" a="1"/>
		<FontHeight val="14"/>
		<Anchors>
			<Anchor point="CENTER" relativeTo="lootwindow" x="0" y="-50" />
		</Anchors>
		<Shadow>
			<Offset x="1" y="1" />
			<Color r="0" g="0" b="0"/>
		</Shadow>
	  </FontString>
	  
	  <FontString name="fade_out_text" font="fonts\ARIALN.ttf" text="BROKEN TEXT" justifyH="LEFT" justifyV="TOP" parent = "lootwindow">
		<Color r="1" g="1" b="1" a="1"/>
		<FontHeight val="14"/>
		<Anchors>
			<Anchor point="CENTER" relativeTo="lootwindow" x="0" y="-100" />
		</Anchors>
		<Shadow>
			<Offset x="1" y="1" />
			<Color r="0" g="0" b="0"/>
		</Shadow>
	  </FontString>
	  
	  <FontString name="stay_text" font="fonts\ARIALN.ttf" text="BROKEN TEXT" justifyH="LEFT" justifyV="TOP" parent = "lootwindow">
		<Color r="1" g="1" b="1" a="1"/>
		<FontHeight val="14"/>
		<Anchors>
			<Anchor point="CENTER" relativeTo="lootwindow" x="0" y="-150" />
		</Anchors>
		<Shadow>
			<Offset x="1" y="1" />
			<Color r="0" g="0" b="0"/>
		</Shadow>
	  </FontString>
	  
	   <FontString name="icon_size_text" font="fonts\ARIALN.ttf" text="BROKEN TEXT" justifyH="LEFT" justifyV="TOP" parent = "lootwindow">
		<Color r="1" g="1" b="1" a="1"/>
		<FontHeight val="14"/>
		<Anchors>
			<Anchor point="CENTER" relativeTo="lootwindow" x="0" y="-200" />
		</Anchors>
		<Shadow>
			<Offset x="1" y="1" />
			<Color r="0" g="0" b="0"/>
		</Shadow>
	  </FontString>
	  
	   <FontString name="font_size_text" font="fonts\ARIALN.ttf" text="BROKEN TEXT" justifyH="LEFT" justifyV="TOP" parent = "lootwindow">
		<Color r="1" g="1" b="1" a="1"/>
		<FontHeight val="14"/>
		<Anchors>
			<Anchor point="CENTER" relativeTo="lootwindow" x="0" y="-250" />
		</Anchors>
		<Shadow>
			<Offset x="1" y="1" />
			<Color r="0" g="0" b="0"/>
		</Shadow>
	  </FontString>
	  
	  </Layer>
	 </Layers>
	 
	 <frames>
	 
	 <EditBox name="fade_in_time" inherits="InputBoxTemplate" autoFocus="false" numeric="true" parent = "lootwindow" multiline = "true" visible = "true">
			<Size x="120" y="36"/>
				<Anchors>
					<Anchor point="CENTER" relativePoint="CENTER">
						<Offset x="0" y="-70"/>
					</Anchor>
				</Anchors>
		 </EditBox>
		 
		  <EditBox name="fade_out_time" inherits="InputBoxTemplate" autoFocus="false" numeric="true" parent = "lootwindow" multiline = "true" visible = "true">
			<Size x="120" y="36"/>
				<Anchors>
					<Anchor point="CENTER" relativePoint="CENTER">
						<Offset x="0" y="-120"/>
					</Anchor>
				</Anchors>
		 </EditBox>
		 
		 <EditBox name="stay_time" inherits="InputBoxTemplate" autoFocus="false" numeric="true" parent = "lootwindow" multiline = "true" visible = "true">
			<Size x="120" y="36"/>
				<Anchors>
					<Anchor point="CENTER" relativePoint="CENTER">
						<Offset x="0" y="-170"/>
					</Anchor>
				</Anchors>
		 </EditBox>
	 
		 <EditBox name="font_size_editbox" inherits="InputBoxTemplate" autoFocus="false" numeric="true" parent = "lootwindow" multiline = "true" visible = "true">
			<Size x="120" y="36"/>
				<Anchors>
					<Anchor point="CENTER" relativePoint="CENTER">
						<Offset x="0" y="-270"/>
					</Anchor>
				</Anchors>
		 </EditBox>
		 
		  <EditBox name="icon_size_editbox" inherits="InputBoxTemplate" autoFocus="false" numeric="true" parent = "lootwindow" multiline = "true" visible = "true">
			<Size x="120" y="36"/>
				<Anchors>
					<Anchor point="CENTER" relativePoint="CENTER">
						<Offset x="0" y="-220"/>
					</Anchor>
				</Anchors>
		 </EditBox>
	 </frames>
</Frame>
	  
 <Button name="Lootwindow_lockbutton" inherits="OptionsButtonTemplate" text="BROKEN TEXT" parent = "lootwindow">
  <Anchors>
    <Anchor point="CENTER">
      <Offset x="0" y="0"/>
    </Anchor> 
  </Anchors>
  <Scripts>
    <OnClick> 
	save_display_settings()
	</OnClick>
  </Scripts>
</Button>
   
 </Ui>