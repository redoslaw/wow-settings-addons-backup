	<Button name="HoloFriendsList_NameButtonTemplate" movable="true" virtual="true">
		<Size><AbsDimension x="295" y="16"/></Size>
		<Layers>
			<Layer level="BORDER">
				<Texture name="$parentWaitIcon" file="Interface\MiniMap\POIIcons">
